{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:node": "vitest -c vitest.config.node.ts",
    "test:unit": "vitest run src/lib src/stores -c vitest.config.node.ts",
    "test:components": "vitest run src/components src/hooks",
    "test:performance": "vitest run src/__tests__/config-performance.test.ts --reporter=verbose",
    "test:perf": "npm run test:performance",
    "test:all": "npm run test:unit && npm run test:components && npm run test:performance"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.5",
    "@types/howler": "^2.2.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "howler": "^2.2.4",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.19.4",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "happy-dom": "^18.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "puppeteer": "^24.11.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^6.1.4",
    "vitest": "^1.6.0"
  }
